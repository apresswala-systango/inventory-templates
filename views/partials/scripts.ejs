<script type="text/javascript">
    var cust_email;
    var handlers = {};
    handlers.init_task = async function(data) {
        ZOHO.CRM.UI.Resize({
            height: "800",
            width: "1200"
        }).then(function(data) {
            console.log(data);
        });
        console.log(data.EntityId[0])
        window.location.href = window.location.origin + "/inventory-templates/" + data.EntityId[0] + '/viewQuote';
    };
    ZOHO.embeddedApp.on("PageLoad", handlers.init_task);
    ZOHO.embeddedApp.init().then(function() {
        console.log('iniittt...')
    });

    $(document).ready(function() {
        var path = window.location.pathname;
        console.log(path);
        $("a").removeClass("active");
        $("a[href='" + path + "']").addClass("active");
    });

    $(function() {
        $(".links a").click(function() {
            $("a").removeClass("active");
            $(this).addClass("active");
        });
        $(".dopdf a").click(function() {
            $(".download-pdf").removeClass("download-div");
            setTimeout(function() {
                $(".download-pdf").addClass("download-div");
            }, 30000);
        });
    });
</script>